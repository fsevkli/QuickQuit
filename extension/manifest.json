{
  "name": "QuickQuit",
  "version": "1.0",
  "description": "A button used to quickly delete a website's browsing history.",
  "permissions": ["activeTab", "tabs", "history"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": [
      "*://*/*"
    ]
  },
  "manifest_version": 3
}
