{
  "manifest_version": 3,
  "name": "Quick Quit",
  "version": "1.0",
  "description": "An extension to remove specified history entries and redirect users.",
  "optional_permissions": ["scripting", "tabs", "history"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "qqLogo16.png",
      "48": "qqLogo48.png",
      "128": "qqLogo128.png"
    }
  },
  "icons": {
    "16": "qqLogo16.png",
    "48": "qqLogo48.png",
    "128": "qqLogo128.png"
  }
}
