{
  "name": "QuickQuit",
  "version": "1.0",
  "description": "A button used to quickly delete a website's browsing history.",
  "permissions": [
    "tabs",
    "history",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "manifest_version": 3
}